<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="444.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.registro.RegistrarprodController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="602.0" style="-fx-background-color: #EFC90A;">
         <children>
            <Label text="REGISTRA EL PRODUCTO">
               <font>
                  <Font name="Arial Rounded MT Bold" size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox layoutX="-2.0" layoutY="100.0" prefHeight="299.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="299.0" prefWidth="296.0" spacing="20.0">
               <children>
                  <Label text="ID producto">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtId" onKeyTyped="#keyEvent">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </TextField>
                  <Label text="Nombre producto">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtNom" onKeyTyped="#keyEvent" prefHeight="31.0" prefWidth="269.0">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </TextField>
                  <Label text="Fecha de lote">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtLote" disable="true" editable="false">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="299.0" prefWidth="304.0" spacing="35.0">
               <children>
                  <Label text="Fecha de vencimiento">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtVence" disable="true" editable="false">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </TextField>
                  <Label text="Precio unitario">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtPrecio" onKeyTyped="#keyEvent">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="btnRegistrar" layoutX="492.0" layoutY="405.0" mnemonicParsing="false" onAction="#actionEvent" prefHeight="25.0" prefWidth="85.0" style="-fx-background-radius: 90; -fx-border-radius: 90;" text="Registrar">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="btnBack" layoutX="393.0" layoutY="405.0" mnemonicParsing="false" onAction="#actionEvent" prefHeight="25.0" prefWidth="85.0" style="-fx-background-radius: 90; -fx-border-radius: 90;" text="Regresar">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
